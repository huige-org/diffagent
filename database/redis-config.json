{
  "redis": {
    "host": "localhost",
    "port": 6379,
    "password": "${REDIS_PASSWORD}",
    "db": 0,
    "keyPrefix": "diffagent:",
    "connectionTimeout": 10000,
    "commandTimeout": 5000,
    "maxRetriesPerRequest": 3,
    "retryDelayOnFailover": 100,
    "enableOfflineQueue": true,
    "tls": false,
    "cacheConfig": {
      "analysisResults": {
        "ttl": 3600,
        "maxMemory": "100mb"
      },
      "recommendations": {
        "ttl": 7200,
        "maxMemory": "50mb"
      },
      "userSessions": {
        "ttl": 86400,
        "maxMemory": "20mb"
      }
    }
  }
}