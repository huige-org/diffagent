# Production Environment Variables
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Performance Settings
MAX_WORKERS=4
CACHE_SIZE=100mb
CACHE_TTL=3600

# Security Settings
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW=900
RATE_LIMIT_MAX=100

# Database/Storage (if applicable)
DATA_DIR=/app/data
LOG_DIR=/app/logs
CONFIG_DIR=/app/config

# Health Check
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10

# Memory Management
MAX_MEMORY_USAGE=800mb
GC_THRESHOLD=500mb